{
  "name": "ga-font-subsets",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "fonts:download": "node ./scripts/download-fonts.js",
    "fonts:download:roboto": "node ./scripts/download-fonts.js -u 'https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap'",
    "fonts:download:garamond-gb": "node ./scripts/download-fonts.js -u 'https://fonts.googleapis.com/css2?family=EB+Garamond:ital,wght@0,400;0,500;0,600;0,700;0,800;1,400;1,500;1,600;1,700;1,800&display=swap'",
    "fonts:download:roboto-condensed": "node ./scripts/download-fonts.js -u 'https://fonts.googleapis.com/css2?family=Roboto+Condensed:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap'"
  },
  "devDependencies": {
    "puppeteer": "^21.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "ts-pattern": "^5.0.6"
  }
}
